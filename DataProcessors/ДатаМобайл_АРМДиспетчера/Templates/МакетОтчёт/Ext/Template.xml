		НомерСтроки,
		ШтрихКод,
		СерийныйНомер,
		Ячейка,
		КодТовара,
		УпаковочныйЛист,
		НазваниеТовара,
		Количество,
		Пользователь,
		Номенклатура,
		ХарактеристикаНоменклатуры,
		ЕдиницаИзмерения,
		ЯчейкаСсылка
	),
	ДатаМобайл_ДокументыТСД.СобранныеДанныеПриемка.(
		Ссылка,
		НомерСтроки,
		ШтрихКод,
		СерийныйНомер,
		Ячейка,
		КодТовара,
		УпаковочныйЛист,
		НазваниеТовара,
		Количество,
		Пользователь,
		Номенклатура,
		ХарактеристикаНоменклатуры,
		ЕдиницаИзмерения,
		ЯчейкаСсылка
	),
	ДатаМобайл_ДокументыТСД.Предопределенный,
	ДатаМобайл_ДокументыТСД.ИмяПредопределенныхДанных
ИЗ
	Справочник.ДатаМобайл_ДокументыТСД КАК ДатаМобайл_ДокументыТСД
ГДЕ
	ДатаМобайл_ДокументыТСД.ДатаЗавершенияСбора &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
	И ДатаМобайл_ДокументыТСД.ДатаЗавершенияСбора &gt;= &amp;МояДатаС 
	И ДатаМобайл_ДокументыТСД.ДатаЗавершенияСбора &lt;= &amp;МояДатаДо</query>
