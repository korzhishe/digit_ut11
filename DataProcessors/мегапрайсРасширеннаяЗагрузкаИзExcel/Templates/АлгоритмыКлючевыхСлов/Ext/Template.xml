		КоличествоКлючевыхСлов = КоличествоКлючевыхСлов + 1;
		
		СтрКлючевоеСлово = СокрЛП(МассивКлючевыхСлов[инд]);
		
		ЕстьБуква = 0;
		ЕстьЦифра = 0;
		
		Для Сч = 1 по СтрДлина(СтрКлючевоеСлово) Цикл	
			ТекСимв = Сред(СтрКлючевоеСлово, Сч, 1);
			
			Если ЕстьЦифра = 0 Тогда
				Если Найти(ПравильныеСимволы, ТекСимв) &gt; 0 Тогда
					ЕстьЦифра = 1;
				КонецЕсли;
			КонецЕсли;
			
			Если ЕстьБуква = 0 Тогда
				Если Найти(ПравильныеСимволыБуквы, ТекСимв) &gt; 0 Тогда
					ЕстьБуква = 1;
				КонецЕсли;
			КонецЕсли;	
		КонецЦикла;
		
		Если ЕстьБуква = 0 И ЕстьЦифра = 0 Тогда
			Продолжить;
		КонецЕсли;
        Значение = Значение+" "+СтрКлючевоеСлово;

	КонецЦикла;
