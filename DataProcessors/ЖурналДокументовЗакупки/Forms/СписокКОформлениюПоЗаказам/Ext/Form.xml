				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.ЗаказПоставщику КАК ЗаказПоставщику
ПОМЕСТИТЬ ВтОстаткиПоЗаказу
ИЗ
	РегистрНакопления.ЗаказыПоставщикам.Остатки(, ЗаказПоставщику ССЫЛКА Документ.ЗаказПоставщику {(Склад) КАК Склад}) КАК Таблица
ГДЕ
	Таблица.КОформлениюОстаток &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.ЗаказПоставщику КАК ЗаказПоставщику
ПОМЕСТИТЬ ВтЕстьНакладные
ИЗ
	ВтОстаткиПоЗаказу КАК Таблица
ГДЕ
	Таблица.ЗаказПоставщику В (
		ВЫБРАТЬ
			ЗаказыПоставщикам.ЗаказПоставщику
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам КАК ЗаказыПоставщикам
		ГДЕ
			ЗаказыПоставщикам.Активность
			И ЗаказыПоставщикам.КОформлению &gt; 0
			И ЗаказыПоставщикам.ВидДвижения = ЗНАЧЕНИЕ(ВидДВиженияНакопления.Расход)
	)
ИНДЕКСИРОВАТЬ ПО
	ЗаказПоставщику
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.ЗаказПоставщику КАК Ссылка,
	МАКСИМУМ(ВЫБОР
			КОГДА НЕ ТаблицаЕстьНакладные.ЗаказПоставщику ЕСТЬ NULL
				ТОГДА 2
			ИНАЧЕ 1
		КОНЕЦ) КАК СостояниеНакладной
ПОМЕСТИТЬ ВтСостояниеНакладной
ИЗ
	ВтОстаткиПоЗаказу КАК Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ ВтЕстьНакладные КАК ТаблицаЕстьНакладные
		ПО (ТаблицаЕстьНакладные.ЗаказПоставщику = Таблица.ЗаказПоставщику)

СГРУППИРОВАТЬ ПО
	Таблица.ЗаказПоставщику

ИНДЕКСИРОВАТЬ ПО
	Таблица.ЗаказПоставщику
;

